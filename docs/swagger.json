                    "application/json"
                ],
                "tags": [
                    "sleep"
                ],
                "summary": "Get list of sleep information",
                "parameters": [
                    {
                        "type": "string",
                        "format": "string",
                        "description": "next list search by next_token",
                        "name": "next_token",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "format": "string",
                        "description": "previous list search by previous_token",
                        "name": "previous_token",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/main.Sleeps"
                        }
                    },
                    "500": {
